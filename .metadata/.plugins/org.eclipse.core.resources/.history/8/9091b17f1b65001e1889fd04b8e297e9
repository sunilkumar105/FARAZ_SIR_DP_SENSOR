#include "ADP8XX_DP_SENSOR.h"

HAL_StatusTypeDef dp_sens_init(void) {
	HAL_StatusTypeDef ret;
	ret = HAL_I2C_IsDeviceReady(&DP_SENS_handletype_def, DP_SENSOR_ADDR, 2,
			i2c_RETRY_TIME); // is device avaialble ?
	if (ret != HAL_OK) {
		return HAL_ERROR;
	}
	return HAL_OK;
}
